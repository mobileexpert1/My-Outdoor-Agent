            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_8sdp"
                android:background="@drawable/corner_round_rectangle"
                android:elevation="0.5dp"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvQuestions"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    android:layout_marginVertical="@dimen/_10sdp"
                    android:fontFamily="@font/nunito_sans_bold"
                    android:text="@string/questions"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />


                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:background="@color/light_white_green" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingHorizontal="@dimen/_13sdp"
                    android:paddingVertical="@dimen/_12sdp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal">


                        <LinearLayout
                            android:id="@+id/llContactPropertyAgent"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:orientation="vertical">

                            <TextView
                                android:id="@+id/tvContactPropertyAgent"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/nunito_sans_semibold"
                                android:gravity="left"
                                android:text="@string/contact_proprty_agent"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_10ssp" />


                            <TextView
                                android:id="@+id/tvContactPAgent"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:drawablePadding="@dimen/_2sdp"
                                android:fontFamily="@font/nunito_sans_regular"
                                android:text="@string/phone_contact"
                                android:textColor="@color/green"
                                android:textSize="@dimen/_10ssp"
                                app:drawableLeftCompat="@drawable/phone_g" />


                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:orientation="horizontal">

                        <LinearLayout
                            android:id="@+id/llTime"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_marginLeft="@dimen/_7sdp"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/nunito_sans_semibold"
                                android:gravity="left"
                                android:text="@string/time"
                                android:textColor="@color/black"
                                android:textSize="@dimen/_10ssp" />


                            <TextView
                                android:id="@+id/tvTime"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/nunito_sans_semibold"
                                android:text="@string/eight_pm"
                                android:textColor="@color/home_screen_grey"
                                android:textSize="@dimen/_9ssp"
                                app:drawableLeftCompat="@drawable/green_small_dot" />


                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:paddingHorizontal="@dimen/_13sdp"
                    android:paddingVertical="@dimen/_12sdp">

                    <TextView
                        android:id="@+id/tvDescriptionPA"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:gravity="left"
                        android:text="@string/leave_us_a_message"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_10ssp" />

                    <EditText
                        android:id="@+id/edtDescriptionPA"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_70sdp"
                        android:layout_marginTop="@dimen/_4sdp"
                        android:layout_toRightOf="@+id/view2"
                        android:padding="@dimen/_2sdp"
                        android:background="@drawable/light_green_box_button_shape"
                        android:hint=""
                        android:visibility="gone"
                        android:gravity="top"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:imeOptions="actionDone"
                        android:inputType="text"
                         />

                    <TextView
                        android:id="@+id/tvSubmitPA"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginBottom="@dimen/_5sdp"
                        android:background="@drawable/login_register_shape_green"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:paddingHorizontal="@dimen/_25sdp"
                        android:paddingVertical="@dimen/_4sdp"
                        android:text="@string/submit"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_9ssp" />

                </LinearLayout>

            </LinearLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="@dimen/_10sdp"
                android:layout_marginVertical="@dimen/_10sdp"
                android:fontFamily="@font/nunito_sans_bold"
                android:gravity="left"
                android:text="@string/similr_prprties"
                android:textColor="@color/black"
                android:textSize="@dimen/_10ssp" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rvSimilarPropertiesHV"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/llAdvantureSeekers"
                android:orientation="horizontal"
                android:padding="@dimen/_5sdp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:listitem="@layout/licence_now_similar_properties" />


            <TextView
                android:id="@+id/tvSimilarProperties"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:gravity="left"
                android:fontFamily="@font/nunito_sans_semibold"
                android:text="@string/no_similar_properties_available"
                android:textColor="@color/home_screen_grey"
                android:textSize="@dimen/_11ssp" />
        </LinearLayout>

    </ScrollView>




</RelativeLayout>