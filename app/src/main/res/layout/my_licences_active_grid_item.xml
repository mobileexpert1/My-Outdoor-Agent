<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/_275sdp"
    android:layout_height="@dimen/_340sdp"
    android:background="@color/profile_grey"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">


    <LinearLayout
        android:id="@+id/llActiveGridItem"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/llSearchHome"
            android:elevation="0.5dp"
            android:padding="@dimen/_6sdp"
            android:layout_marginBottom="@dimen/_8sdp"
            android:background="@drawable/corner_round_rectangle"
            android:orientation="vertical">


            <FrameLayout
                android:id="@+id/image_logo"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_90sdp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/ivDataImage"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_90sdp"
                    android:layout_weight="4"
                    android:background="@drawable/corner_round_rectangle"
                    android:clipToOutline="true"
                    android:scaleType="fitXY"
                    android:src="@drawable/ic_logo" />

                <!--  <androidx.appcompat.widget.AppCompatImageView
                      android:id="@+id/iv_camera"
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content" />-->

                <TextView
                    android:id="@+id/tvActiveRLU"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_8ssp"
                    android:elevation="@dimen/_1sdp"
                    android:visibility="gone"
                    android:fontFamily="@font/nunito_sans_bold"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingBottom="@dimen/_2sdp"
                    android:layout_gravity="left"
                    android:text="@string/active_rlu"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="@drawable/ic_green_tie"/>

                <TextView
                    android:id="@+id/tvActivePermit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_8ssp"
                    android:elevation="@dimen/_1sdp"
                    android:visibility="visible"
                    android:fontFamily="@font/nunito_sans_bold"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingBottom="@dimen/_2sdp"
                    android:layout_gravity="left"
                    android:text="@string/active_permit"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="@drawable/ic_green_tie"/>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:weightSum="1">
                    <TextView
                        android:id="@+id/tvMembershipPermit"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_6ssp"
                        android:background="@drawable/ic_yellow_tie"
                        android:elevation="@dimen/_1sdp"
                        android:layout_weight="0.4"
                        android:visibility="visible"
                        android:paddingStart="@dimen/_2sdp"
                        android:fontFamily="@font/nunito_sans_bold"
                        android:gravity="left|center_vertical"
                        android:paddingBottom="@dimen/_2sdp"
                        android:layout_gravity="left"
                        android:layout_marginRight="@dimen/_10sdp"
                        android:text="@string/membership_permit"/>
                    <View
                        android:layout_weight="0.6"
                        android:layout_height="wrap_content"
                        android:layout_width="0dp" />

                </LinearLayout>


                <TextView
                    android:id="@+id/tvMembershipRLU"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:visibility="gone"
                    android:textSize="@dimen/_8ssp"
                    android:elevation="@dimen/_1sdp"
                    android:fontFamily="@font/nunito_sans_bold"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/_10sdp"
                    android:paddingBottom="@dimen/_2sdp"
                    android:layout_gravity="left"
                    android:text="@string/membership_rlu"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="@drawable/ic_yellow_tie"/>

                <TextView
                    android:id="@+id/tvPendingPermit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_8ssp"
                    android:elevation="@dimen/_1sdp"
                    android:fontFamily="@font/nunito_sans_bold"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/_5sdp"
                    android:visibility="gone"
                    android:paddingBottom="@dimen/_2sdp"
                    android:layout_gravity="left"
                    android:text="@string/pending_permit"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="@drawable/ic_red_tie"/>

                <TextView
                    android:id="@+id/tvPendingRLU"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_8ssp"
                    android:elevation="@dimen/_1sdp"
                    android:visibility="gone"
                    android:fontFamily="@font/nunito_sans_bold"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingBottom="@dimen/_2sdp"
                    android:layout_gravity="left"
                    android:text="@string/pending_rlu"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="@drawable/ic_red_tie"/>

                <TextView
                    android:id="@+id/tvExpiredRLU"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_8ssp"
                    android:elevation="@dimen/_1sdp"
                    android:visibility="gone"
                    android:fontFamily="@font/nunito_sans_bold"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingBottom="@dimen/_2sdp"
                    android:layout_gravity="left"
                    android:text="@string/expired_rlu"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="@drawable/ic_red_tie"/>

                <TextView
                    android:id="@+id/tvExpiredPermit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_8ssp"
                    android:elevation="@dimen/_1sdp"
                    android:visibility="gone"
                    android:fontFamily="@font/nunito_sans_bold"
                    android:gravity="left|center_vertical"
                    android:paddingLeft="@dimen/_5sdp"
                    android:paddingBottom="@dimen/_2sdp"
                    android:layout_gravity="left"
                    android:text="@string/expired_permit"
                    android:layout_marginRight="@dimen/_30sdp"
                    android:background="@drawable/ic_red_tie"/>



            </FrameLayout>

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_6sdp"
                android:layout_weight="4"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/tvHeadDisplayName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/nunito_sans_semibold"
                    android:gravity="left"
                    android:maxLines="1"
                    android:minLines="1"
                    android:text="@string/hpl_sales"
                    android:textColor="@color/black"
                    android:textSize="@dimen/_10ssp" />


                <TextView
                    android:id="@+id/tvStateNameAndcountyName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/tvHeadDisplayName"
                    android:layout_marginTop="5dp"
                    android:fontFamily="@font/nunito_sans_semibold"
                    android:text="@string/clark_country"
                    android:textColor="@color/home_screen_grey"
                    android:textSize="@dimen/_10ssp" />


            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16sdp"
                android:orientation="horizontal"
                android:layout_marginBottom="@dimen/_8sdp">

                <LinearLayout
                    android:id="@+id/llLicencesTerms"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:layout_alignParentLeft="true"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/tvAdventureSeeker"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:gravity="left"
                        android:text="@string/property_licence_terms"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_10ssp" />


                    <TextView
                        android:id="@+id/tvStartDateToEndDate"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tvAdventureSeeker"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:maxLines="3"
                        android:text="@string/july2022"
                        android:textColor="@color/home_screen_grey"
                        android:textSize="@dimen/_9ssp" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_alignParentRight="true"
                    android:layout_weight="1"
                    android:layout_toRightOf="@+id/llLicencesTerms"
                    android:gravity="right"
                    android:orientation="vertical">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:gravity="right"
                        android:paddingRight="@dimen/_2sdp"
                        android:text="@string/acres"
                        android:textColor="@color/black"
                        android:textSize="@dimen/_10ssp" />


                    <TextView
                        android:id="@+id/tvMAcres"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingRight="@dimen/_2sdp"
                        android:layout_below="@+id/tvAdventureSeeker"
                        android:gravity="right"
                        android:fontFamily="@font/nunito_sans_semibold"
                        android:text="@string/fouronefive"
                        android:textColor="@color/home_screen_grey"
                        android:textSize="@dimen/_10ssp" />

                </LinearLayout>

            </LinearLayout>


        </LinearLayout>

        <LinearLayout
            android:id="@+id/llLicenceViewDetails"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/llSearchHome"
            android:background="@drawable/green_button_shape"
            android:orientation="horizontal">
            <TextView
                android:textSize="@dimen/_9ssp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_9sdp"
                android:fontFamily="@font/nunito_sans_bold"
                android:gravity="center"
                android:text="@string/view_details"
                android:textColor="@color/white" />
        </LinearLayout>
<!--
  <LinearLayout
            android:id="@+id/llLicenceViewDetails"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/llSearchHome"
            android:background="@drawable/green_border_shape"
            android:orientation="horizontal">
            <TextView
                android:textSize="@dimen/_9ssp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_11sdp"
                android:fontFamily="@font/nunito_sans_bold"
                android:gravity="center"
                android:text="@string/view_details"
                android:textColor="@color/green_border_button_color" />
        </LinearLayout>
-->

        <LinearLayout
            android:id="@+id/llExpiredLicence"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/llSearchHome"
            android:background="@drawable/grey_border_shape"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/tvExpired"
                android:textSize="@dimen/_9ssp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="@dimen/_9sdp"
                android:fontFamily="@font/nunito_sans_bold"
                android:gravity="center"
                android:text="EXPIRED"
                android:textColor="@color/home_screen_grey" />
        </LinearLayout>

    </LinearLayout>

</LinearLayout>
